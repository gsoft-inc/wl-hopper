{
    "Space": {
        "inset": {
            "xs": {
                "$value": "{space.2}",
                "$type": "number",
                "group": "spacing"
            },
            "sm": {
                "$value": "{space.3}",
                "$type": "number",
                "group": "spacing"
            },
            "md": {
                "$value": "{space.4}",
                "$type": "number",
                "group": "spacing"
            },
            "lg": {
                "$value": "{space.5}",
                "$type": "number",
                "group": "spacing"
            },
            "xl": {
                "$value": "{space.6}",
                "$type": "number",
                "group": "spacing"
            }
        },
        "inset squish": {
