                "group": "spacing"
            },
            "md": {
                "$value": "0 {space.4} 0 0",
                "$type": "number",
                "group": "spacing"
            },
            "lg": {
                "$value": "0 {space.5} 0 0",
                "$type": "number",
                "group": "spacing"
            },
            "xl": {
                "$value": "0 {space.6} 0 0",
                "$type": "number",
                "group": "spacing"
            }
