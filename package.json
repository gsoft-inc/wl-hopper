{
    "name": "wl-hopper",
    "version": "0.0.0",
    "keywords": [],
    "author": {
        "name": "Workleap"
    },
    "private": true,
    "type": "module",
    "sideeffects": false,
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gsoft-inc/wl-hopper.git"
    },
    "packageManager": "pnpm@8.6.2",
    "scripts": {
        "doc:start": "pnpm --filter=docs dev",
        "doc:storybook": "pnpm --filter=docs storybook",
        "doc:build": "pnpm --filter=docs build",
        "test": "jest",
        "build:tools": "pnpm --filter ./tooling/* -r --parallel build",
        "build": "pnpm --filter=\"@hopper-ui/*\" -r --parallel build ",
        "changeset": "changeset",
        "ci-release": "pnpm build && changeset publish",
        "lint": "pnpm run \"/^lint:.*/\"",
        "lint:eslint": "eslint . --max-warnings=-1 --cache --cache-location node_modules/.cache/eslint",
        "lint:style": "stylelint \"**/*.css\" --allow-empty-input --cache --cache-location node_modules/.cache/stylelint",
        "clean": "pnpm -r --parallel --include-workspace-root exec pnpm dlx rimraf dist node_modules/.cache",
        "reset": "pnpm clean && pnpm reset:modules",
        "reset:modules": "pnpm -r --parallel --include-workspace-root exec pnpm dlx rimraf node_modules pnpm-lock.yaml",
        "storybook": "pnpm build && storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "dependencies": {
        "@netlify/plugin-nextjs": "4.36.0",
        "storybook": "7.2.0"
    },
    "devDependencies": {
        "@babel/core": "7.22.5",
        "@hopper-ui/storybook-addon": "workspace:*",
        "@storybook/addon-essentials": "7.2.0",
        "@storybook/addon-interactions": "7.2.0",
        "@storybook/addon-links": "7.2.0",
        "@storybook/addon-mdx-gfm": "7.2.0",
        "@storybook/blocks": "7.2.0",
        "@storybook/react": "7.2.0",
        "@storybook/react-webpack5": "7.2.0",
        "@storybook/testing-library": "0.2.0",
        "@types/jest": "29.5.2",
        "@types/react": "18.2.12",
        "@types/react-dom": "18.2.5",
        "@workleap/eslint-plugin": "1.8.1",
        "@workleap/stylelint-config": "1.2.1",
        "@workleap/typescript-configs": "2.3.1",
        "jest": "29.2.2",
        "prop-types": "15.8.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "stylelint": "15.7.0",
        "ts-node": "10.9.1",
        "tsconfig-paths-webpack-plugin": "4.0.1",
        "typescript": "5.1.6",
        "eslint-plugin-storybook": "0.6.13",
        "prettier": "2.4.1"
    },
    "bugs": {
        "url": "https://github.com/gsoft-inc/wl-hopper/issues"
    },
    "readme": "ERROR: No README data found!",
    "homepage": "https://github.com/gsoft-inc/wl-hopper#readme",
    "_id": "wl-hopper@0.0.0"
}
